section( id='download' class='container content-section text-center' )
  div( class='row' )
    div( class='col-lg-8 col-lg-offset-2' )
      h2=t('section.download.h2')
      ul( class='list-inline banner-social-buttons' )
        li
          a( href='zip/squadjs.zip' class='btn btn-default btn-lg' target='_blank' download='squadjs.zip' title='Package with documentation and examples')=t('section.download.zip') 
        li
          a( href='https://github.com/LCluber/Squad.js' class='btn btn-default btn-lg' target='_blank' title='Sources for the entire project')
            i( class='fa fa-github fa-fw' )
            =t('section.download.github')
